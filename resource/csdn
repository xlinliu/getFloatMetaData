<script>
$('#good_at').bind('click',function(){
	if($("#checkuserflag").attr("isLogin") == "true"){
		judgeHelpGroup();
	}else if($("#checkuserflag").attr("isLogin") == "false"){
			$(".opacity").show();
			$("#checkuserflag").show();
			$(".dialog1").addClass("current").show();
			dialog_scroll(".fixed");
			$(window).scroll();
	}else{
		addLoginCookie();
		$.get("/user/checkLogin?iask_cookie="+cookies_startWith("iask_cookie")+"&m=" + Math.random(),function(data){
			var json = null;
			try{json = $.parseJSON(data);}catch (e) {};
			if(json.succ == "Y"){
				$("#checkuserflag").attr("isLogin",true);
				$("#checkuserflag").attr("userId",json.jsonData.userId);
				$("#ask_tab").attr("uid",json.jsonData.userId);
				judgeHelpGroup();
			}else{
				$("#checkuserflag").attr("isLogin",false);
				$(".opacity").show();
	 			$("#checkuserflag").show();
	 			$(".dialog1").addClass("current").show();
	 			dialog_scroll(".fixed");
	 			$(window).scroll();
			}
		});
	}
});
$('.goodat_layer .close_btn').bind('click',function(){
	$('.goodat_layer').hide();
});
$('.goodat_layer .green_btn').bind('click',function(){
	$('.goodat_layer').hide();
	var cid = $("#currentCid").attr("categoryId");
	window.location.href="/help/join?from=&cid="+cid;
});
$('.askbox_layer .close_btn').bind('click',function(){
	$('.askbox_layer').hide();
});
$(".adv_select").on("click",".askAdviser",function(e){
	var me = $(this);
	var aid = me.attr("action_data");
	$("#user_card").hide();
	if($("#checkuserflag").attr("isLogin") == "true"){
		postAsk();
	}else if($("#checkuserflag").attr("isLogin") == "false"){
		$(".opacity").show();
			$("#checkuserflag").show();
			$(".dialog1").addClass("current").show();
			dialog_scroll(".fixed");
			$(window).scroll();
	}else{
		addLoginCookie();
		$.get("/user/checkLogin?iask_cookie="+cookies_startWith("iask_cookie")+"&m=" + Math.random(),function(data){
			var json = null;
			try{json = $.parseJSON(data);}catch (e) {};
			if(json.succ == "Y"){
				$("#checkuserflag").attr("isLogin",true);
				$("#checkuserflag").attr("userId",json.jsonData.userId);
				$("#ask_tab").attr("uid",json.jsonData.userId);
				postAsk();
			}else{
				$("#checkuserflag").attr("isLogin",false);
				$(".opacity").show();
	 			$("#checkuserflag").show();
	 			$(".dialog1").addClass("current").show();
	 			dialog_scroll(".fixed");
	 			$(window).scroll();
			}
		});
	}
	function postAsk(){
		$.post("/adviser/preask",{aid:aid},function(result){
			if(result){
				if(result.status == 0){
					var _data = result.data;
					_data.aid = aid;
					$.adviserAskAlert.call(me,e,_data);
				}else{
					$.adviserDiamondsLackAlert.call(me,e);
				}
			}else{
				alertErrorMsg("操作失败，请稍后重试！");
			};
		},"json");
	}
});
</script>
