<script type="text/javascript">
$(function(){
	$('.tab_bar a').bind('click',function(){
		var nb = $(this).index();
		var $ct = $(this).parent(".tab_bar").next('.tab_ct').find(".tab_div");
		$(this).addClass("current").siblings().removeClass("current");
		$ct.eq(nb).siblings().hide();
		$ct.eq(nb).show();
		return false;
	});
	$('.search-tab span').on('click',function(){
		var nb = $(this).index();
		var $ct = $(this).parent(".search-tab").next('.search-ct').find(".search-item");
		$(this).addClass("current").siblings().removeClass("current");
		$ct.eq(nb).siblings().hide();
		$ct.eq(nb).show();
	});
});
</script>
